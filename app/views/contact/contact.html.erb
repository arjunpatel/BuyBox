<div class="Contact contact">  <h2>Contact Us</h2>  <%= form_for(:product, :url => {:action => 'create'}) do |f| %>      <table summary="Product form fields">      <tr>        <td align = right>Full Name:&nbsp;&nbsp;</td>        <td><%= f.text_field(:full_name) %></td>      </tr>      <tr>        <td align = right>Email Address:&nbsp;&nbsp;</td>        <td><%= f.text_field(:email) %></td>      </tr>	  <tr>        <td align = right>Message:&nbsp;&nbsp;</td>		<td align = center><%= f.text_area :message, :cols =>70, :rows => 10%></td>      </tr>    </table>	          <div class="form-buttons">      <%= submit_tag("Back") %>	  <%= submit_tag("Send") %>    </div>      <% end %></div>
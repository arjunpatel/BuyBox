<div class="Contact contact">  <h2>Contact Us</h2>  <%= form_for(:email, :url => {:action => 'sendmail'}) do |f| %>      <table summary="Product form fields">      <tr>        <td align = right>Full Name:&nbsp;&nbsp;</td>        <td><%= f.text_field :fullname %></td>      </tr>      <tr>        <td align = right>Email Address:&nbsp;&nbsp;</td>        <td><%= f.text_field :sender %></td>      </tr>	  <tr>        <td align = right>Message:&nbsp;&nbsp;</td>		<td align = center><%= f.text_area :message%></td>      </tr>    </table>	          <div class="form-buttons">      <%= submit_tag("Back") %>	  <%= submit_tag("Send") %>    </div>      <% end %></div>